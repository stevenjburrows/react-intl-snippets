{
	"Intl Formatted Message": {
		"prefix": "fmsg",
		"body": "<FormattedMessage id='${1}' defaultMessage='${2}' />${0}",
		"description": "React Intl Formatted Message"
	},
	"Intl Formatted Message with value": {
		"prefix": "fmsgv",
		"body": [
			"<FormattedMessage",
			"id='${1}'",
			"defaultMessage='${2}<${4:b}>${3}</${4:b}>${5}'",
			"values={{",
			"    ${4:b}: chunks => (",
			"        <${6:span} className='${7}'>{chunks}</${6:span}>",
			"    ),",
			"}}",
			"/>${0}"
		],
		"description": "React Intl Formatted Message with values"
	},
	"Intl Formatted Date": {
		"prefix": "fdate",
		"body": "<FormattedDate value={${1:new Date()} />${0}",
		"description": "React Intl Formatted Date"
	},
	"Intl Formatted Date Parts": {
		"prefix": "fdp",
		"body": [
			"<FormattedDateParts>",
			"value={${1:new Date()}}",
			"year=\"${2:numeric}\"",
			"month=\"${3:numeric}\"",
			"day=\"${4:numeric}\"",
			"</FormattedDateParts>${0}"
		],
		"description": "React Intl Formatted Date Parts"
	},
	"Intl Formatted Time": {
		"prefix": "ftime",
		"body": "<FormattedTime value={${1:new Date()}} />${0}",
		"description": "React Intl Formatted Time"
	},
	"Intl Formatted Time with parts": {
		"prefix": "ftp",
		"body": [
			"<FormattedTimeParts value={${1: new Date()}} >",
			"{parts => (",
			"<>",
			"{parts[0].value}",
			"{parts[1].value}",
			"{parts[2].value}",
			"</>",
			")}",
			"</FormattedTimeParts>${0}"
		],
		"description": "React Intl Formatted Time with parts"
	}
}